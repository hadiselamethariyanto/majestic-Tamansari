<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bwx.tamansari.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_navigation_home_to_daftarWisataFragment"
            app:destination="@id/navigation_daftar_wisata" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_homestay"
            app:destination="@id/navigation_homestay" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_travel_package"
            app:destination="@id/navigation_travel_package" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_restaurant"
            app:destination="@id/navigation_restaurant" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_peta"
            app:destination="@id/navigation_peta" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_news"
            app:destination="@id/navigation_news" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.bwx.tamansari.ui.riwayat.RiwayatFragment"
        android:label="@string/title_transaksi"
        tools:layout="@layout/fragment_riwayat" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.bwx.tamansari.ui.pesan.PesanFragment"
        android:label="@string/title_pesan"
        tools:layout="@layout/fragment_pesan" />
    <fragment
        android:id="@+id/navigation_daftar_wisata"
        android:name="com.bwx.tamansari.ui.wisata.list.DaftarWisataFragment"
        android:label="@string/toolbar_wisata"
        tools:layout="@layout/fragment_daftar_wisata">

        <action
            android:id="@+id/action_navigation_daftar_wisata_to_navigation_detail_wisata"
            app:destination="@id/navigation_detail_wisata" />
    </fragment>
    <fragment
        android:id="@+id/navigation_detail_wisata"
        android:name="com.bwx.tamansari.ui.wisata.detail.DetailWisataFragment"
        android:label="@string/navigation_detail_wisata"
        tools:layout="@layout/fragment_detail_wisata">
        <action
            android:id="@+id/action_navigation_detail_wisata_to_navigation_choose_ticket_wisata"
            app:destination="@id/navigation_choose_ticket_wisata" />
    </fragment>

    <fragment
        android:id="@+id/navigation_homestay"
        android:name="com.bwx.tamansari.ui.homestay.list.HomestayFragment"
        android:label="@string/navigation_list_homestay"
        tools:layout="@layout/fragment_homestay">
        <action
            android:id="@+id/action_navigation_homestay_to_navigation_detail_homestay"
            app:destination="@id/navigation_detail_homestay" />
    </fragment>

    <fragment
        android:id="@+id/navigation_travel_package"
        android:name="com.bwx.tamansari.ui.paket.list.PaketFragment"
        android:label="@string/navigation_list_travel_package"
        tools:layout="@layout/fragment_paket">
        <action
            android:id="@+id/action_navigation_travel_package_to_navigation_detail_travel_packaage"
            app:destination="@id/navigation_detail_travel_packaage" />
    </fragment>
    <fragment
        android:id="@+id/navigation_restaurant"
        android:name="com.bwx.tamansari.ui.restaurant.list.RestaurantFragment"
        android:label="@string/navigation_list_restaurant"
        tools:layout="@layout/fragment_restaurant">
        <action
            android:id="@+id/action_navigation_restaurant_to_navigation_detail_restaurant"
            app:destination="@id/navigation_detail_restaurant" />
    </fragment>
    <fragment
        android:id="@+id/navigation_peta"
        android:name="com.bwx.tamansari.ui.peta.PetaFragment"
        android:label="@string/navigation_maps"
        tools:layout="@layout/fragment_peta" />
    <fragment
        android:id="@+id/navigation_news"
        android:name="com.bwx.tamansari.ui.berita.list.BeritaFragment"
        android:label="@string/navigation_news"
        tools:layout="@layout/fragment_berita">
        <action
            android:id="@+id/action_navigation_news_to_navigation_detail_news"
            app:destination="@id/navigation_detail_news" />
    </fragment>
    <fragment
        android:id="@+id/navigation_detail_news"
        android:name="com.bwx.tamansari.ui.berita.detail.DetailBeritaFragment"
        android:label="@string/navigation_detail_news"
        tools:layout="@layout/fragment_detail_berita" />
    <fragment
        android:id="@+id/navigation_detail_homestay"
        android:name="com.bwx.tamansari.ui.homestay.detail.DetailHomestayFragment"
        android:label="@string/navigation_detail_homestay"
        tools:layout="@layout/fragment_detail_homestay">
        <action
            android:id="@+id/action_navigation_detail_homestay_to_navigation_choose_room"
            app:destination="@id/navigation_choose_room" />
    </fragment>
    <fragment
        android:id="@+id/navigation_detail_travel_packaage"
        android:name="com.bwx.tamansari.ui.paket.detail.DetailTravelPackageFragment"
        android:label="@string/navigation_detail_travel_package"
        tools:layout="@layout/fragment_detail_travel_package">
        <action
            android:id="@+id/action_navigation_detail_travel_packaage_to_navigation_choose_travel_package"
            app:destination="@id/navigation_choose_travel_package" />
    </fragment>
    <fragment
        android:id="@+id/navigation_detail_restaurant"
        android:name="com.bwx.tamansari.ui.restaurant.detail.DetailRestaurantFragment"
        android:label="@string/navigation_detail_restaurant"
        tools:layout="@layout/fragment_detail_restaurant" />
    <fragment
        android:id="@+id/navigation_account"
        android:name="com.bwx.tamansari.ui.akun.AkunFragment"
        android:label="fragment_akun"
        tools:layout="@layout/fragment_akun" />
    <fragment
        android:id="@+id/navigation_choose_ticket_wisata"
        android:name="com.bwx.tamansari.ui.wisata.choose_ticket.ChooseTicketWisataFragment"
        android:label="@string/navigation_choose_ticket"
        tools:layout="@layout/fragment_choose_ticket_wisata">
        <action
            android:id="@+id/action_navigation_choose_ticket_wisata_to_navigation_review_transaction_wisata"
            app:destination="@id/navigation_review_transaction_wisata" />
    </fragment>

    <fragment
        android:id="@+id/navigation_review_transaction_wisata"
        android:name="com.bwx.tamansari.ui.wisata.review.ReviewTransactionWisataFragment"
        android:label="@string/navigation_review_transaction"
        tools:layout="@layout/fragment_review_transaction_wisata" />
    <fragment
        android:id="@+id/navigation_choose_room"
        android:name="com.bwx.tamansari.ui.homestay.choose_room.ChooseRoomFragment"
        android:label="@string/navigation_choose_room"
        tools:layout="@layout/fragment_choose_room">
        <action
            android:id="@+id/action_navigation_choose_room_to_navigation_review_transaction_homestay"
            app:destination="@id/navigation_review_transaction_homestay" />
    </fragment>
    <fragment
        android:id="@+id/navigation_review_transaction_homestay"
        android:name="com.bwx.tamansari.ui.homestay.review_transaction.ReviewTransactionHomestayFragment"
        android:label="@string/navigation_review_transaction"
        tools:layout="@layout/fragment_review_transaction_homestay" />

    <fragment
        android:id="@+id/navigation_choose_travel_package"
        android:name="com.bwx.tamansari.ui.paket.choose_paket.ChooseTravelPackageFragment"
        android:label="@string/navigation_choose_travel_package"
        tools:layout="@layout/fragment_choose_travel_package" >
        <action
            android:id="@+id/action_navigation_choose_travel_package_to_navigation_review_transaction_travel_package"
            app:destination="@id/navigation_review_transaction_travel_package" />
    </fragment>
    <fragment
        android:id="@+id/navigation_review_transaction_travel_package"
        android:name="com.bwx.tamansari.ui.paket.review.ReviewTransactionTravelPackageFragment"
        android:label="@string/navigation_review_transaction"
        tools:layout="@layout/fragment_review_transaction_travel_package" />


</navigation>